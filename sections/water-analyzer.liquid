    }
    bonus = Math.min(bonus, 5);

    const penaliteTotale = penaliteBacterio + penaliteTurbidite + penaliteChimique + penaliteChlore;
    const scoreFinal = Math.min(100, Math.max(0, scoreBase - penaliteTotale + bonus));

