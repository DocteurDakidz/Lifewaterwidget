<!-- Leaflet CSS pour la carte -->
{% if section.settings.enable_map %}
<link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" />
{% endif %}

<script>
(function() {
  'use strict';
  
  // Configuration avec syst√®me de fallback CORS optimis√©
  const CORS_METHODS = [
    {
      name: 'AllOrigins',
      proxy: 'https://api.allorigins.win/get?url=',
      parse: function(data) { return JSON.parse(data.contents); },
      timeout: 8000
    },
    {
      name: 'ProxyAnywhereHeroku', 
      proxy: 'https://cors-anywhere.herokuapp.com/',
      parse: function(data) { return data; },
      timeout: 6000
    },
    {
      name: 'ThingProxy',
      proxy: 'https://thingproxy.freeboard.io/fetch/',
      parse: function(data) { return data; },
      timeout: 6000
    },
    {
      name: 'Direct',
      proxy: '',
      parse: function(data) { return data; },
      timeout: 4000
    }
  ];

  // Configuration
  const CONFIG = {
    sectionId: '{{ section.id }}',
    enableMap: {{ section.settings.enable_map | default: false }},
    maxRetries: 2,
    cacheTimeout: 600000, // 10 minutes
    dateRangeYears: {{ section.settings.analysis_years | default: 3 }},
    proximityRadius: {{ section.settings.proximity_radius | default: 50 }}, // km
    productsCollection: '{{ section.settings.products_collection }}',
    shopDomain: '{{ shop.permanent_domain }}'
  };

  // Variables globales
  let map = null;
  let marker = null;
  let selectedAddress = null;
  let submittedAddress = null;
  const cache = new Map();

  // S√©lecteurs DOM
  const selectors = {
    formSection: '[data-form-section]',
    contactForm: '.water-analyzer__contact-form',
    analysisSection: '[data-analysis-section]',
    analysisInput: '[data-analysis-input]',
    analysisButton: '[data-analysis-button]',
    loading: '[data-loading]',
    loadingText: '[data-loading-text]',
    progressFill: '[data-progress-fill]',
    progressText: '[data-progress-text]',
    results: '[data-results]',
    examples: '[data-example]',
    mapToggle: '[data-map-toggle]',
    mapContainer: '[data-map-container]',
    mapElement: '[data-map]',
    mapClose: '[data-map-close]',
    mapInfo: '[data-map-info]',
    mapConfirm: '[data-map-confirm]',
    selectedAddressSpan: '[data-selected-address]'
  };

  // Utilitaires
  const utils = {
    sanitizeHtml: function(str) {
      const div = document.createElement('div');
      div.textContent = str;
      return div.innerHTML;
    },

    formatDate: function(dateString) {
      return new Date(dateString).toLocaleDateString('fr-FR', {
        year: 'numeric',
        month: 'long',
        day: 'numeric'
      });
    },

    getCacheKey: function(address) {
      return 'water_analysis_' + address.toLowerCase().replace(/\s+/g, '_');
    },

    calculateDistance: function(lat1, lon1, lat2, lon2) {
      const R = 6371; // Rayon de la Terre en km
      const dLat = (lat2 - lat1) * Math.PI / 180;
      const dLon = (lon2 - lon1) * Math.PI / 180;
      const a = Math.sin(dLat/2) * Math.sin(dLat/2) +
                Math.cos(lat1 * Math.PI / 180) * Math.cos(lat2 * Math.PI / 180) *
                Math.sin(dLon/2) * Math.sin(dLon/2);
      const c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1-a));
      return R * c;
    }
  };

  // Syst√®me de scoring EXACT de la version pr√©c√©dente
  const scoringSystem = {
    calculateScore: function(analyses, location) {
      if (!analyses || analyses.length === 0) {
        return {
          score: 50,
          label: 'Donn√©es insuffisantes',
          color: '#ffc107',
          details: 'Aucune analyse disponible pour calculer un score pr√©cis'
        };
      }

      let totalScore = 0;
      let weights = {
        bacteriological: 0.4,  // 40% - S√©curit√© sanitaire
        physicochemical: 0.35, // 35% - Qualit√© chimique  
        freshness: 0.15,       // 15% - Fra√Æcheur des donn√©es
        availability: 0.1      // 10% - Disponibilit√© des donn√©es
      };

      // 1. Score bact√©riologique (40%)
      const bactConform = analyses.filter(a => a.conformite_limites_bact_prelevement === 'C').length;
      const bactTotal = analyses.filter(a => a.conformite_limites_bact_prelevement).length;
      const bactScore = bactTotal > 0 ? (bactConform / bactTotal) * 100 : 50;

      // 2. Score physico-chimique (35%)
      const physConform = analyses.filter(a => a.conformite_limites_pc_prelevement === 'C').length;
      const physTotal = analyses.filter(a => a.conformite_limites_pc_prelevement).length;
      const physScore = physTotal > 0 ? (physConform / physTotal) * 100 : 50;

      // 3. Score de fra√Æcheur (15%)
      const latestDate = new Date(analyses[0].date_prelevement);
      const daysSinceLatest = (Date.now() - latestDate.getTime()) / (1000 * 60 * 60 * 24);
      let freshnessScore = 100;
      if (daysSinceLatest > 365) freshnessScore = 70;
      else if (daysSinceLatest > 180) freshnessScore = 85;
      else if (daysSinceLatest > 90) freshnessScore = 95;

      // 4. Score de disponibilit√© (10%)
      const availabilityScore = Math.min(100, (analyses.length / 10) * 100);

      // Calcul final pond√©r√©
      totalScore = (
        bactScore * weights.bacteriological +
        physScore * weights.physicochemical +
        freshnessScore * weights.freshness +
        availabilityScore * weights.availability
      );

      // D√©termination du label et couleur
      let label, color;
      if (totalScore >= 90) {
        label = 'Excellente';
        color = '#28a745';
      } else if (totalScore >= 75) {
        label = 'Tr√®s bonne';
        color = '#6f42c1';
      } else if (totalScore >= 60) {
        label = 'Bonne';
        color = '#17a2b8';
      } else if (totalScore >= 45) {
        label = 'Correcte';
        color = '#ffc107';
      } else if (totalScore >= 30) {
        label = 'M√©diocre';
        color = '#fd7e14';
      } else {
        label = 'Mauvaise';
        color = '#dc3545';
      }

      return {
        score: Math.round(totalScore),
        label: label,
        color: color,
        details: {
          bacteriological: Math.round(bactScore),
          physicochemical: Math.round(physScore),
          freshness: Math.round(freshnessScore),
          availability: Math.round(availabilityScore),
          totalAnalyses: analyses.length,
          latestDate: latestDate
        }
      };
    }
  };

  // Gestion de la carte
  const mapManager = {
    init: function() {
      if (!CONFIG.enableMap) return;
      
      if (typeof L === 'undefined') {
        const script = document.createElement('script');
        script.src = 'https://unpkg.com/leaflet@1.9.4/dist/leaflet.js';
        script.onload = function() {
          mapManager.setupMap();
        };
        document.head.appendChild(script);
      } else {
        mapManager.setupMap();
      }
    },

    setupMap: function() {
      const mapElement = document.querySelector(selectors.mapElement);
      if (!mapElement) return;

      map = L.map(mapElement.id).setView([46.603354, 1.888334], 6);
      
      L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
        attribution: '¬© OpenStreetMap contributors'
      }).addTo(map);

      map.on('click', mapManager.onMapClick);
    },

    onMapClick: function(e) {
      const lat = e.latlng.lat;
      const lng = e.latlng.lng;
      
      if (marker) {
        map.removeLayer(marker);
      }
      
      marker = L.marker([lat, lng]).addTo(map);
      mapManager.reverseGeocode(lat, lng);
    },

    reverseGeocode: function(lat, lng) {
      const url = 'https://api-adresse.data.gouv.fr/reverse/?lon=' + lng + '&lat=' + lat;
      
      fetchWithFallback(url).then(function(data) {
        if (data && data.features && data.features.length > 0) {
          const address = data.features[0].properties.label;
          selectedAddress = address;
          
          const addressEl = document.querySelector(selectors.selectedAddressSpan);
          const mapInfo = document.querySelector(selectors.mapInfo);
          
          if (addressEl && mapInfo) {
            addressEl.textContent = address;
            mapInfo.style.display = 'block';
          }
        }
      }).catch(function(error) {
        console.warn('Erreur geocodage inverse:', error);
      });
    },

    show: function() {
      const container = document.querySelector(selectors.mapContainer);
      if (container) {
        container.style.display = 'block';
        
        setTimeout(function() {
          if (map) {
            map.invalidateSize();
          }
        }, 100);
      }
    },

    hide: function() {
      const container = document.querySelector(selectors.mapContainer);
      if (container) {
        container.style.display = 'none';
      }
      
      selectedAddress = null;
      const mapInfo = document.querySelector(selectors.mapInfo);
      if (mapInfo) {
        mapInfo.style.display = 'none';
      }
    },

    confirmAddress: function() {
      if (selectedAddress) {
        const input = document.querySelector(selectors.analysisInput);
        if (input) {
          input.value = selectedAddress;
        }
        mapManager.hide();
        
        setTimeout(function() {
          analyzeWater(selectedAddress);
        }, 300);
      }
    }
  };

  // Gestion de l'interface
  const ui = {
    updateProgress: function(percent, step) {
      const progressFill = document.querySelector(selectors.progressFill);
      const progressText = document.querySelector(selectors.progressText);
      const loadingText = document.querySelector(selectors.loadingText);
      
      if (progressFill) progressFill.style.width = percent + '%';
      if (progressText) progressText.textContent = percent + '%';
      if (loadingText && step) loadingText.textContent = step;
    },

    showLoading: function(step) {
      step = step || 'Initialisation...';
      const loading = document.querySelector(selectors.loading);
      const results = document.querySelector(selectors.results);
      
      if (results) results.style.display = 'none';
      if (loading) loading.style.display = 'block';
      
      ui.updateProgress(0, step);
    },

    hideLoading: function() {
      const loading = document.querySelector(selectors.loading);
      if (loading) loading.style.display = 'none';
    },

    toggleButton: function(disabled, text) {
      const button = document.querySelector(selectors.analysisButton);
      if (!button) return;

      button.disabled = disabled;
      
      if (text) {
        const buttonText = button.querySelector('.water-analyzer__button-text');
        const buttonIcon = button.querySelector('.water-analyzer__button-icon');
        
        if (buttonText) buttonText.textContent = text;
        if (buttonIcon) buttonIcon.textContent = disabled ? 'üîÑ' : 'üîç';
      }
    },

    showResults: function(html) {
      const results = document.querySelector(selectors.results);
      if (results) {
        results.innerHTML = html;
        results.style.display = 'block';
        
        setTimeout(function() {
          results.scrollIntoView({ 
            behavior: 'smooth', 
            block: 'start' 
          });
        }, 100);
      }
    },

    showAnalysisSection: function(address) {
      const formSection = document.querySelector(selectors.formSection);
      const analysisSection = document.querySelector(selectors.analysisSection);
      const analysisInput = document.querySelector(selectors.analysisInput);
      
      if (formSection) formSection.style.display = 'none';
      if (analysisSection) analysisSection.style.display = 'block';
      if (analysisInput && address) {
        analysisInput.value = address;
      }
      
      // Lancer automatiquement l'analyse
      if (address) {
        setTimeout(function() {
          analyzeWater(address);
        }, 500);
      }
    }
  };

  // Fonction de fetch avec timeout
  function fetchWithTimeout(url, options) {
    options = options || {};
    const timeout = options.timeout || 5000;
    const fetchOptions = Object.assign({}, options);
    delete fetchOptions.timeout;
    
    const controller = new AbortController();
    const timeoutId = setTimeout(function() {
      controller.abort();
    }, timeout);
    
    fetchOptions.signal = controller.signal;
    
    return fetch(url, fetchOptions).then(function(response) {
      clearTimeout(timeoutId);
      return response;
    }).catch(function(error) {
      clearTimeout(timeoutId);
      throw error;
    });
  }

  // Fonction de fetch avec fallback
  function fetchWithFallback(url, methodIndex, retryCount) {
    methodIndex = methodIndex || 0;
    retryCount = retryCount || 0;
    
    if (methodIndex >= CORS_METHODS.length) {
      throw new Error('Toutes les m√©thodes de connexion ont √©chou√©. V√©rifiez votre connexion internet.');
    }

    const method = CORS_METHODS[methodIndex];
    const proxyUrl = method.proxy ? method.proxy + encodeURIComponent(url) : url;
    
    return fetchWithTimeout(proxyUrl, {
      method: 'GET',
      headers: {
        'Accept': 'application/json',
        'User-Agent': 'WaterAnalyzer/2.0'
      },
      mode: 'cors',
      timeout: method.timeout
    }).then(function(response) {
      if (!response.ok) {
        throw new Error('HTTP ' + response.status + ': ' + response.statusText);
      }
      return response.json();
    }).then(function(data) {
      return method.parse(data);
    }).catch(function(error) {
      if (retryCount < CONFIG.maxRetries) {
        return new Promise(function(resolve) {
          setTimeout(resolve, 1000 * (retryCount + 1));
        }).then(function() {
          return fetchWithFallback(url, methodIndex, retryCount + 1);
        });
      }
      
      return fetchWithFallback(url, methodIndex + 1, 0);
    });
  }

  // Fonction d'analyse principale
  function analyzeWater(address) {
    if (!address || !address.trim()) {
      throw new Error('Veuillez saisir une adresse valide');
    }

    const cacheKey = utils.getCacheKey(address);
    
    if (cache.has(cacheKey)) {
      const cached = cache.get(cacheKey);
      if (Date.now() - cached.timestamp < CONFIG.cacheTimeout) {
        return Promise.resolve(cached.data);
      }
    }

    ui.showLoading('√âtape 1: Localisation de l\'adresse...');
    ui.updateProgress(10, 'G√©ocodage en cours...');
    
    const addressUrl = 'https://api-adresse.data.gouv.fr/search/?q=' + encodeURIComponent(address) + '&limit=5';
    
    return fetchWithFallback(addressUrl).then(function(locationData) {
      if (!locationData || !locationData.features || !locationData.features.length) {
        throw new Error('Adresse non trouv√©e. Essayez avec le code postal ou le nom de la ville.');
      }

      const location = locationData.features[0];
      const cityCode = location.properties.citycode;
      const coordinates = location.geometry.coordinates;
      
      ui.updateProgress(30, '√âtape 2: Recherche des analyses locales...');
      
      const dateLimit = new Date();
      dateLimit.setFullYear(dateLimit.getFullYear() - CONFIG.dateRangeYears);
      const dateLimitStr = dateLimit.toISOString().split('T')[0];

      const waterUrl = 'https://hubeau.eaufrance.fr/api/v1/qualite_eau_potable/resultats_dis?code_commune=' + cityCode + '&date_min_prelevement=' + dateLimitStr + '&size=200&format=json&sort=desc';
      
      return fetchWithFallback(waterUrl).then(function(qualityData) {
        ui.updateProgress(60, '√âtape 3: Analyse des donn√©es...');

        // Si pas de donn√©es locales, recherche √©largie
        if (!qualityData || !qualityData.data || qualityData.data.length === 0) {
          ui.updateProgress(70, 'Recherche √©largie dans la r√©gion...');
          return expandedSearch(location, dateLimit);
        }

        ui.updateProgress(90, 'Finalisation de l\'analyse...');

        const result = {
          location: location,
          qualityData: qualityData,
          isExpanded: false,
          timestamp: Date.now()
        };

        cache.set(cacheKey, {
          data: result,
          timestamp: Date.now()
        });

        ui.updateProgress(100, 'Analyse termin√©e !');
        return result;
      });
    });
  }

  // Recherche √©largie g√©ographique
  function expandedSearch(location, dateLimit) {
    const coordinates = location.geometry.coordinates;
    const lon = coordinates[0];
    const lat = coordinates[1];
    
    // Recherche dans un rayon √©largi
    const bbox = {
      minLon: lon - 0.5,
      maxLon: lon + 0.5,
      minLat: lat - 0.5,
      maxLat: lat + 0.5
    };

    const expandedUrl = 'https://hubeau.eaufrance.fr/api/v1/qualite_eau_potable/resultats_dis?' +
      'bbox=' + bbox.minLon + ',' + bbox.minLat + ',' + bbox.maxLon + ',' + bbox.maxLat +
      '&date_min_prelevement=' + dateLimit.toISOString().split('T')[0] +
      '&size=500&format=json&sort=desc';

    return fetchWithFallback(expandedUrl).then(function(expandedData) {
      if (expandedData && expandedData.data && expandedData.data.length > 0) {
        // Filtrer par proximit√©
        const nearbyAnalyses = expandedData.data.filter(function(analysis) {
          if (!analysis.coordonnee_x || !analysis.coordonnee_y) return false;
          
          const distance = utils.calculateDistance(
            lat, lon,
            parseFloat(analysis.coordonnee_y), parseFloat(analysis.coordonnee_x)
          );
          
          return distance <= CONFIG.proximityRadius;
        });

        if (nearbyAnalyses.length > 0) {
          return {
            location: location,
            qualityData: { data: nearbyAnalyses },
            isExpanded: true,
            expandedRadius: CONFIG.proximityRadius,
            timestamp: Date.now()
          };
        }
      }

      // Aucune donn√©e trouv√©e m√™me en recherche √©largie
      return {
        location: location,
        qualityData: { data: [] },
        isExpanded: true,
        timestamp: Date.now()
      };
    });
  }

  // R√©cup√©ration des produits Shopify
  function getProductSuggestions(score) {
    if (!CONFIG.productsCollection) return Promise.resolve([]);

    const productsUrl = '/collections/' + CONFIG.productsCollection + '/products.json';
    
    return fetch(productsUrl).then(function(response) {
      if (!response.ok) throw new Error('Collection non trouv√©e');
      return response.json();
    }).then(function(data) {
      if (!data.products) return [];
      
      // Filtrer et trier les produits selon le score
      let filteredProducts = data.products;
      
      if (score < 60) {
        // Score faible : privil√©gier les syst√®mes de filtration
        filteredProducts = filteredProducts.filter(function(product) {
          const title = product.title.toLowerCase();
          return title.includes('filtre') || title.includes('purifi') || title.includes('osmose');
        });
      } else if (score < 80) {
        // Score moyen : carafe filtrante, adoucisseur
        filteredProducts = filteredProducts.filter(function(product) {
          const title = product.title.toLowerCase();
          return title.includes('carafe') || title.includes('adoucis') || title.includes('anti-calcaire');
        });
      }
      
      return filteredProducts.slice(0, 3);
    }).catch(function(error) {
      console.warn('Erreur r√©cup√©ration produits:', error);
      return [];
    });
  }

  // G√©n√©ration du HTML des r√©sultats avec scoring
  function generateResultsHtml(location, qualityResponse, isExpanded) {
    const props = location.properties;
    let html = '';

    // Informations sur l'adresse
    html += '<div class="water-result water-result--location">';
    html += '<h3 class="water-result__title">üìç Adresse analys√©e</h3>';
    html += '<div class="water-result__content">';
    html += '<p><strong>' + utils.sanitizeHtml(props.label) + '</strong></p>';
    html += '<p>üèòÔ∏è ' + utils.sanitizeHtml(props.city) + ' (' + (props.postcode || 'N/A') + ')</p>';
    html += '<p>üÜî Code INSEE: ' + props.citycode + '</p>';
    if (props.context) {
      html += '<p>üìç ' + utils.sanitizeHtml(props.context) + '</p>';
    }
    if (isExpanded) {
      html += '<p>üîç <em>Donn√©es r√©gionales (rayon ' + CONFIG.proximityRadius + 'km)</em></p>';
    }
    html += '</div></div>';

    if (qualityResponse && qualityResponse.data && qualityResponse.data.length > 0) {
      const analyses = qualityResponse.data;
      const scoreData = scoringSystem.calculateScore(analyses, location);
      const latest = analyses[0];
      const lastDate = utils.formatDate(latest.date_prelevement);

      // Score principal avec cercle
      html += '<div class="water-result water-result--score">';
      html += '<h3 class="water-result__title">üèÜ Score Qualit√©</h3>';
      html += '<div class="water-result__content">';
      html += '<div class="water-score" style="--score-percent: ' + scoreData.score + '%; --score-color: ' + scoreData.color + ';">';
      html += '<div class="water-score__circle">';
      html += '<span class="water-score__value">' + scoreData.score + '</span>';
      html += '</div>';
      html += '<span class="water-score__label">' + scoreData.label + '</span>';
      html += '</div>';
      html += '<p><strong>Bas√© sur ' + analyses.length + ' analyses</strong> sur ' + CONFIG.dateRangeYears + ' ans</p>';
      html += '</div></div>';

      // Informations g√©n√©rales
      html += '<div class="water-result water-result--quality">';
      html += '<h3 class="water-result__title">üìä Rapport Qualit√© - ' + utils.sanitizeHtml(props.city) + '</h3>';
      html += '<div class="water-result__content">';
      html += '<p><strong>Derni√®re analyse:</strong> ' + lastDate + '</p>';
      html += '<p><strong>Total analyses:</strong> ' + analyses.length + '</p>';
      if (latest.nom_distributeur) {
        html += '<p><strong>Distributeur:</strong> ' + utils.sanitizeHtml(latest.nom_distributeur) + '</p>';
      }
      if (latest.nom_installation) {
        html += '<p><strong>Installation:</strong> ' + utils.sanitizeHtml(latest.nom_installation) + '</p>';
      }
      html += '</div></div>';

      // D√©tails de conformit√©
      const bactConform = analyses.filter(function(d) { return d.conformite_limites_bact_prelevement === 'C'; }).length;
      const bactTotal = analyses.filter(function(d) { return d.conformite_limites_bact_prelevement; }).length;
      
      const physConform = analyses.filter(function(d) { return d.conformite_limites_pc_prelevement === 'C'; }).length;
      const physTotal = analyses.filter(function(d) { return d.conformite_limites_pc_prelevement; }).length;

      if (bactTotal > 0 || physTotal > 0) {
        html += '<div class="water-result water-result--conformity">';
        html += '<h4 class="water-result__title">‚úÖ D√©tail de Conformit√©</h4>';
        html += '<div class="water-result__content">';

        if (bactTotal > 0) {
          const bactPercent = (bactConform / bactTotal) * 100;
          html += '<p>ü¶† <strong>Qualit√© bact√©riologique:</strong> ' + bactPercent.toFixed(1) + '% conforme (' + bactConform + '/' + bactTotal + ' analyses)</p>';
        }

        if (physTotal > 0) {
          const physPercent = (physConform / physTotal) * 100;
          html += '<p>‚öóÔ∏è <strong>Param√®tres physico-chimiques:</strong> ' + physPercent.toFixed(1) + '% conforme (' + physConform + '/' + physTotal + ' analyses)</p>';
        }

        html += '</div></div>';
      }

      // Recommandations bas√©es sur le score
      html += '<div class="water-result water-result--recommendations">';
      html += '<h4 class="water-result__title">üí° Recommandations</h4>';
      html += '<div class="water-result__content">';
      
      if (scoreData.score >= 80) {
        html += '<p>‚úÖ <strong>Excellente qualit√© !</strong> Votre eau respecte toutes les normes.</p>';
        html += '<p>‚Ä¢ üíß Continuez √† boire l\'eau du robinet en toute confiance</p>';
        html += '<p>‚Ä¢ üå°Ô∏è Conservez l\'eau au frais pour pr√©server sa qualit√©</p>';
      } else if (scoreData.score >= 60) {
        html += '<p>‚ö†Ô∏è <strong>Qualit√© correcte</strong> avec quelques points d\'attention.</p>';
        html += '<p>‚Ä¢ üî¨ Surveillez les prochaines analyses de votre distributeur</p>';
        html += '<p>‚Ä¢ üíß Vous pouvez envisager une carafe filtrante pour am√©liorer le go√ªt</p>';
      } else {
        html += '<p>üö® <strong>Qualit√© pr√©occupante.</strong> Des am√©liorations sont recommand√©es.</p>';
        html += '<p>‚Ä¢ üìû Contactez imm√©diatement votre distributeur d\'eau</p>';
        html += '<p>‚Ä¢ üîß Consid√©rez l\'installation d\'un syst√®me de filtration</p>';
      }
      
      html += '<p>‚Ä¢ üìû En cas de doute, contactez votre distributeur d\'eau</p>';
      html += '<p>‚Ä¢ üíß Consommez 1,5 √† 2L d\'eau par jour selon vos besoins</p>';
      html += '</div></div>';

      // Suggestions de produits (sera rempli par JavaScript)
      if (CONFIG.productsCollection) {
        html += '<div class="water-result water-result--products" id="products-section">';
        html += '<h4 class="water-result__title">üõí Produits recommand√©s</h4>';
        html += '<div class="water-result__content">';
        html += '<p>Chargement des recommandations...</p>';
        html += '</div></div>';
        
        // Charger les produits de mani√®re asynchrone
        getProductSuggestions(scoreData.score).then(function(products) {
          const productsSection = document.getElementById('products-section');
          if (productsSection && products.length > 0) {
            let productsHtml = '<h4 class="water-result__title">üõí Produits recommand√©s</h4>';
            productsHtml += '<div class="water-result__content">';
            productsHtml += '<p>Bas√© sur votre score de ' + scoreData.score + '/100 :</p>';
            productsHtml += '<div class="water-products">';
            
            products.forEach(function(product) {
              const image = product.images && product.images[0] ? product.images[0].src : '';
              const price = product.variants && product.variants[0] ? product.variants[0].price : '';
              const handle = product.handle;
              
              productsHtml += '<a href="/products/' + handle + '" class="water-product">';
              if (image) {
                productsHtml += '<img src="' + image + '" alt="' + utils.sanitizeHtml(product.title) + '" class="water-product__image">';
              }
              productsHtml += '<h5 class="water-product__title">' + utils.sanitizeHtml(product.title) + '</h5>';
              if (price) {
                productsHtml += '<p class="water-product__price">' + (parseFloat(price) / 100).toFixed(2) + ' ‚Ç¨</p>';
              }
              productsHtml += '</a>';
            });
            
            productsHtml += '</div></div>';
            productsSection.innerHTML = productsHtml;
          } else if (productsSection) {
            productsSection.style.display = 'none';
          }
        });
      }

    } else {
      html += '<div class="water-result water-result--warning">';
      html += '<h4 class="water-result__title">‚ö†Ô∏è Aucune analyse disponible</h4>';
