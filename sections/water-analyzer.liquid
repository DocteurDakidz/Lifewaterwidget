  // Gestion de la progression
  const progressManager = {
    steps: [
      { id: 1, duration: 25, text: 'G√©ocodage de l\'adresse...' },
      { id: 2, duration: 35, text: 'R√©cup√©ration des analyses...' },
      { id: 3, duration: 25, text: 'Calcul du score de qualit√©...' },
      { id: 4, duration: 15, text: 'G√©n√©ration des recommandations...' }
    ],
    currentStep: 0,
    currentProgress: 0,

    start: function() {
      this.currentStep = 0;
      this.currentProgress = 0;
      this.updateProgress(0);
      this.setStepActive(1);
    },

    nextStep: function(stepId, customText) {
      if (this.currentStep > 0) {
        this.setStepCompleted(this.currentStep);
      }
      
      this.currentStep = stepId;
      this.setStepActive(stepId);
      
      if (customText) {
        this.updateStepText(stepId, customText);
      }
      
      // Calculer le pourcentage cumul√© jusqu'√† cette √©tape
      let cumulativeProgress = 0;
      for (let i = 0; i < stepId - 1; i++) {
        cumulativeProgress += this.steps[i].duration;
      }
      
      this.animateProgress(cumulativeProgress);
    },

    complete: function() {
      if (this.currentStep > 0) {
        this.setStepCompleted(this.currentStep);
      }
      this.animateProgress(100);
    },

    error: function(stepId) {
      this.setStepError(stepId);
    },

    updateProgress: function(percentage) {
      const progressFill = document.querySelector(selectors.progressFill);
      const progressPercentage = document.querySelector(selectors.progressPercentage);
      
      if (progressFill) {
        progressFill.style.width = percentage + '%';
      }
      if (progressPercentage) {
        progressPercentage.textContent = Math.round(percentage) + '%';
      }
    },

    animateProgress: function(targetPercentage) {
      const startPercentage = this.currentProgress;
      const difference = targetPercentage - startPercentage;
      const duration = 800; // ms
      const startTime = Date.now();
      
      const animate = () => {
        const elapsed = Date.now() - startTime;
        const progress = Math.min(elapsed / duration, 1);
        
        // Easing function (ease-out)
        const easeOut = 1 - Math.pow(1 - progress, 3);
        const currentPercentage = startPercentage + (difference * easeOut);
        
        this.updateProgress(currentPercentage);
        
        if (progress < 1) {
          requestAnimationFrame(animate);
        } else {
          this.currentProgress = targetPercentage;
        }
      };
      
      requestAnimationFrame(animate);
    },

    setStepActive: function(stepId) {
      const step = document.querySelector('[data-step="' + stepId + '"]');
      if (step) {
        step.classList.remove('completed', 'error');
        step.classList.add('active');
